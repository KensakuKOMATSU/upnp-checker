{
  "manifest_version": 2,
  "name": "UPnP Checker",
  "description": "UPnP checker",
  "version": "0.1",
  "minimum_chrome_version": "23",
  "app": {
    "background": {
      "scripts": ["lib/util.js", "lib/upnp.js", "main.js"]
    }
  },
  "icons": {
  },
  "permissions": [
    "http://*/",
    "experimental",
    {"socket": [
      "udp-bind",
      "udp-send-to"
    ]}
  ]
}
